// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Utente {
  Id Int @id
  Username String
  SocketId String
  Lobby Lobby @relation(fields: [LobbyId], references: [Id])
  LobbyId String
}

model Lobby {
  Id String @id
  Utenti Utente[] 
}